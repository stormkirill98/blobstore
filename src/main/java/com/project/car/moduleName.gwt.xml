<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='moduleName'>
    <inherits name="com.google.gwt.user.User"/>

    <inherits name="com.google.gwt.inject.Inject"/>

    <!-- Other module inherits -->


    <inherits name="com.gwtplatform.dispatch.Dispatch"/>

    <inherits name="com.gwtplatform.mvp.MvpWithEntryPoint"/>

    <inherits name="com.google.gwt.http.HTTP" />


    <!-- GWT Bootstrap 3 -->
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>

    <extend-configuration-property name="gin.ginjector.modules" value="com.project.car.client.gin.ClientModule"/>

    <!-- Specify the paths for translatable code -->
    <source path="client"/>

    <source path="shared"/>

    <public path='resource'>
        <include name='css/*.css'/>
        <include name='js/*.js'/>
    </public>

    <set-configuration-property name="CssResource.legacy" value = "true" />
    <set-configuration-property name="CssResource.enableGss" value="true"/>
</module>
